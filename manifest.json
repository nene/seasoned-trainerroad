{
  "manifest_version": 2,
  "name": "Seasoned TrainerRoad",
  "version": "1.0",
  "description": "Enhanced experience for trainerroad.com",
  "icons": {
    "16": "icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.trainerroad.com/app/cycling/workouts/*"
  ],
  "background": {
    "scripts": ["tr-background.js"]
  },
  "page_action": {
    "default_icon": {
      "16": "icon.png"
    },
    "default_title": "Edit workout"
  }
}
